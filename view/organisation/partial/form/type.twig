{% do form.prepare() %}
{{ form().openTag(form)|raw }}

{% set type = form.get('organisation_entity_type') %}

{{ lbs5formelement(type.get('id')) }}
{{ lbs5formelement(type.get('type')) }}
{{ lbs5formelement(type.get('description')) }}
{{ lbs5formelement(type.get('standardType')) }}

<div class="row mb-3">
    <div class="offset-sm-3 col-sm-9">
        {{ lbs5formelement(form.get('csrf')) }}
        {{ lbs5formelement(form.get('submit')) }}
        {{ lbs5formelement(form.get('cancel')) }}
    </div>
</div>

{{ form().closeTag()|raw }}
