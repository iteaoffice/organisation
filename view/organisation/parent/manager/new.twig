{% do headTitle().append(translate("txt-admin")) %}
{% do headTitle().append(translate("txt-new-parent")) %}

<h1>{{ translate("txt-new-parent") }}</h1>

{% do form.prepare() %}
{{ form().openTag(form)|raw }}

{% set parent = form.get('organisation_entity_parententity') %}

{{ lbs5formelement(parent.get('id')) }}
{{ lbs5formelement(parent.get('type')) }}
{{ lbs5formelement(parent.get('memberType')) }}
{{ lbs5formelement(parent.get('epossMemberType')) }}
{{ lbs5formelement(parent.get('artemisiaMemberType')) }}

{{ organisationformelement(parent.get('organisation')) }}
{{ contactformelement(parent.get('contact')) }}
{{ lbs5formelement(form.get('csrf')) }}

<div class="row mb-3">
    <div class="offset-sm-3 col-sm-9">

        {{ lbs5formelement(form.get('csrf')) }}
        {{ lbs5formelement(form.get('submit')) }}
        {{ lbs5formelement(form.get('cancel')) }}
    </div>
</div>

{{ form().closeTag()|raw }}