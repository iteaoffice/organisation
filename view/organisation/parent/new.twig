{% do headLink().appendStylesheet(
'//ajax.googleapis.com/ajax/libs/jqueryui/1.10.3/themes/ui-lightness/jquery-ui.min.css'
) %}

{% do headTitle().append(translate("txt-admin")) %}
{% do headTitle().append(translate("txt-new-parent")) %}

<h1 class="display-4">{{ translate("txt-new-parent") }}</h1>

{% do form.prepare() %}
{% do form.setAttribute('class','form-horizontal') %}

{{ form().openTag(form)|raw }}

{% set parent = form.get('organisation_entity_oparent') %}

{{ ztbformelement(parent.get('id')) }}
{{ organisationformelement(parent.get('organisation')) }}

{{ ztbformelement(parent.get('type')) }}
{{ ztbformelement(parent.get('status')) }}
{{ ztbformelement(parent.get('epossMemberType')) }}
{{ ztbformelement(parent.get('artemisiaMemberType')) }}

{{ contactformelement(parent.get('contact')) }}

<div class="form-group row">
    <div class="offset-md-3 col-sm-offset-3 col-sm-9">
        {{ ztbformelement(form.get('submit')) }}
        {{ ztbformelement(form.get('cancel')) }}
    </div>
</div>

{{ form().closeTag()|raw }}

<script type="text/javascript">
    $('input[type=date]').datepicker({
        sliderAccessArgs: {touchonly: false},
        dateFormat: 'yy-mm-dd'
    });
</script>