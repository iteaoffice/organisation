{
  "name": "iteaoffice/organisation",
  "type": "library",
  "license": "proprietary",
  "description": "General code for organisations",
  "keywords": [
    "organisation"
  ],
  "homepage": "https://itea3.org",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.itea3.eu"
    }
  ],
  "authors": [
    {
      "name": "Johan van der Heide",
      "email": "johan.van.der.heide@itea3.org"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.1",
    "zendframework/zendframework": "~3.0",
    "iteaoffice/testing": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "6.0.x-dev"
  },
  "autoload": {
    "psr-4": {
      "Organisation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OrganisationTest\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev",
      "dev-develop": "2.1-dev"
    }
  }
}
