{
  "name": "iteaoffice/organisation",
  "type": "library",
  "license": "proprietary",
  "description": "General code for  Organisations",
  "keywords": [
    "organisation"
  ],
  "homepage": "https://itea3.org",
  "repositories": [
    {
      "type": "composer",
      "url": "http://packages.itea3.eu"
    }
  ],
  "authors": [
    {
      "name": "Johan van der Heide",
      "email": "johan.van.der.heide@itea3.org"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">5.5",
    "zendframework/zendframework": "~2.5",
    "doctrine/doctrine-orm-module": "0.9.*@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "@stable",
    "pdepend/pdepend": "@stable",
    "phpmd/phpmd": "@stable",
    "sebastian/phpcpd": "@stable",
    "squizlabs/php_codesniffer": "@stable",
    "fabpot/php-cs-fixer": "@stable",
    "jakub-onderka/php-parallel-lint": "@stable",
    "jakub-onderka/php-console-highlighter": "@stable"
  },
  "autoload": {
    "psr-4": {
      "Organisation\\": "src/Organisation/",
      "OrganisationTest\\": "tests/OrganisationTest/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1-dev",
      "dev-develop": "1.2-dev"
    }
  }
}
