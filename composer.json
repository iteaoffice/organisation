{
  "name": "iteaoffice/organisation",
  "type": "library",
  "license": "proprietary",
  "description": "Code to manage organisations",
  "keywords": [
    "organisation"
  ],
  "homepage": "https://itea3.org",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.itea4.org"
    }
  ],
  "authors": [
    {
      "name": "Johan van der Heide",
      "email": "johan.van.der.heide@itea3.org"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.4",
    "laminas/laminas-mvc": "^3.1",
    "laminas/laminas-dependency-plugin": "^0.2"
  },
  "require-dev": {
    "iteaoffice/testing": "2.*",
    "iteaoffice/search": "2.*",
    "iteaoffice/contact": "5.*",
    "iteaoffice/general": "5.*",
    "iteaoffice/program": "5.*",
    "iteaoffice/affiliation": "5.*",
    "iteaoffice/project": "5.*",
    "iteaoffice/event": "5.*",
    "iteaoffice/invoice": "5.*",
    "samsonasik/error-hero-module": "2.*",
    "setasign/fpdi-tcpdf": "~2.0",
    "jield-webdev/zf3-bootstrap4": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Organisation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OrganisationTest\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "5.0-DEV",
      "dev-develop": "5.1-DEV"
    }
  },
  "scripts": {
    "check": [
      "@php-lint",
      "@phpstan",
      "@cs"
    ],
    "fix": [
      "@cbf",
      "@cs-fix"
    ],
    "php-lint": "./vendor/bin/parallel-lint src",
    "phpstan": "./vendor/bin/phpstan analyse src/ --level=1",
    "cs": "./vendor/bin/phpcs",
    "cbf": "./vendor/bin/phpcbf",
    "cs-fix": "./vendor/bin/php-cs-fixer fix -v src",
    "test": "./vendor/bin/phpunit"
  }
}
