{
  "name": "iteaoffice/organisation",
  "type": "library",
  "license": "proprietary",
  "description": "General code for organisations",
  "keywords": [
    "organisation"
  ],
  "homepage": "https://itea3.org",
  "repositories": [
    {
      "type": "composer",
      "url": "http://packages.itea3.eu"
    }
  ],
  "authors": [
    {
      "name": "Johan van der Heide",
      "email": "johan.van.der.heide@itea3.org"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.6",
    "zendframework/zendframework": "~2.5 || ~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "@stable",
    "pdepend/pdepend": "@stable",
    "phpmd/phpmd": "@stable",
    "sebastian/phpcpd": "@stable",
    "squizlabs/php_codesniffer": "@stable",
    "friendsofphp/php-cs-fixer": "@stable",
    "jakub-onderka/php-parallel-lint": "@stable",
    "jakub-onderka/php-console-highlighter": "@stable"
  },
  "autoload": {
    "psr-4": {
      "Organisation\\": "src/",
      "OrganisationTest\\": "tests/"
    }
  },
  "config": {
    "secure-http": false
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev",
      "dev-develop": "2.1-dev"
    }
  }
}
