{
    "name": "debranova/organisation",
    "type": "library",
    "license": "proprietary",
    "description": "General code for Debranova Organisations",
    "keywords": ["organisation"],
    "homepage": "http://debranova.org",
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.debranova.org"
        }
    ],
    "authors": [
        {
            "name": "Johan van der Heide",
            "email": "johan.van.der.heide@itea3.org"
        },
        {
            "name": "Andre Hebben",
            "email": "andre.hebben@artemis-ia.eu"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5",
        "zendframework/zendframework": "2.3.*@dev",
        "doctrine/doctrine-orm-module": "0.9.*@dev",
        "gedmo/doctrine-extensions": "2.3.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "4.3.*@dev",
        "squizlabs/php_codesniffer": "2.0.*@dev",
        "fabpot/php-cs-fixer": "@stable",
        "doctrine/data-fixtures": "2.0.*@dev"
    },
    "autoload": {
        "psr-0": {
            "Organisation\\": "src/",
            "OrganisationTest\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev",
            "dev-develop": "1.2-dev"
        }
    }
}
